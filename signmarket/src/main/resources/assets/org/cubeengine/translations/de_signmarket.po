#: de/cubeisland/engine/module/signmarket/MarketSign.java:528
msgid ""
"{text:Sell:color=DARK_BLUE}: {amount} for {input#price} to {input#owner}"
msgstr "{text:Verkaufen:color=DARK_BLUE}: {amount} für {input#price} von {input#owner}"

#: de/cubeisland/engine/module/signmarket/MarketSign.java:533
msgid ""
"{text:Sell:color=DARK_BLUE}: {amount} for {input#price} to {user#owner}"
msgstr "{text:Verkaufen:color=DARK_BLUE}: {amount} für {input#price} von {user#owner}"

